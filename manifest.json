{
  "name": "NAZAS",
    "version": "1.0",
  "description": "NAZAS ;D",
  "web_accessible_resources": [ "/src/jquery.js", "/src/underscore.js", "/src/backbone.js", "/src/backbone.localStorage.js", "/src/todos.js" ], 
  "content_scripts": [
    {
      "matches": ["*://*/*"], 
	  
      "js": ["/src/jquery.js", "/src/page.js"]
    }
  ],
   "permissions": [ "*://*/*",   "webNavigation", "webRequest", "activeTab" ],
  
  "background": {
    "scripts": ["/src/jquery.js", "/src/Enum.js", "/src/Item.js", "/src/background.js"],
    "persistent": true
  },
  "browser_action": {
   "default_icon":  "/img/default.png",
    "default_title": "Pana"
  },
  "options_page": "options.html",
  "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://code.jquery.com/jquery-1.10.2.min.js; object-src 'self' "
}




